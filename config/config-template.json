{
  "prefix": "<YOUR_PREFIX>",
  "token": "<YOUR_TOKEN>",
  "enable_prefix_change": true,

  "enable_logging": true,
  "log_file_name": "combined.log",

  "role_ids_admin": [ "" ],
  "user_ids_admin": [ "" ],

  "enable_activity": true,
  "activity": {
    "name": "<prefix>help",
    "type": "PLAYING"
  },

  "help": {
    "show_only_permitted_commands": false,
    "send_to_dm": false
  },

  "embed": {
    "color": "RANDOM",
    "avatar_url": ""
  },

  "default_lang": "en",
  "enable_lang_change": true,
  "lang_paths": {
    "de": "text_de.json",
    "en": "text_en.json"
  }
}
